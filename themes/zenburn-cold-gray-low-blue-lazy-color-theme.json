    "tokenColors": [
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#87a564ff"
            }
        },
        {
            "name": "preprocessor",
            "scope": "meta.preprocessor, punctuation.definition.preprocessor",
            "settings": {
                "foreground": "#e8b36dff"
            }
        },
        {
            "name": "phpdoc",
            "scope": "keyword.other.phpdoc",
            "settings": {
                "foreground": "#56873aff"
            }
        },
        {
            "name": "comment punctuation (&#x2f;*, #, &#x2f;&#x2f;)",
            "scope": "punctuation.definition.comment, punctuation.whitespace.comment",
            "settings": {
                "foreground": "#a0c579ff"
            }
        },
        {
            "name": "embedded ruby in string delimiters",
            "scope": "punctuation.section.embedded",
            "settings": {
                "foreground": "#898267ff"
            }
        },
        {
            "name": "embedded ruby in string",
            "scope": "source.ruby.embedded",
            "settings": {
                "foreground": "#cc8d70ff"
            }
        },
        {
            "name": "Directive",
            "scope": "keyword.other.directive",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ded3a6ff"
            }
